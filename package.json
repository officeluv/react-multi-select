    "react-addons-test-utils": "15.6.2",
    "react-dom": "15.6.2",
    "sinon": "1.17.7",
    "uglify-js": "2.8.29",
    "webpack": "2.7.0",
    "@storybook/react": "3.4.11",
    "@storybook/addons": "3.4.11",
    "babel-runtime": "6.26.0"
  },
  "peerDependencies": {
    "react": "^0.14.7 || ^15.0.0"
  },
  "dependencies": {
    "fuzzy-match-utils": "^1.2.0"
